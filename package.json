{
  "name": "@notthedom/logging-express",
  "version": "1.0.0",
  "description": "Logging middleware for the express server request object",
  "main": "dist/main.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc",
    "dev": "tsc --watch",
    "server": "nodemon dist/server.js",
    "start": "concurrently --prefix-colors \"white.dim\" --timings \"npm run dev\" \"npm run server\""
  },
  "keywords": [
    "express",
    "middleware",
    "logger",
    "custom",
    "request"
  ],
  "author": "Dom van Almsick <notthedom@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.31",
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "dotenv": "^16.0.0",
    "express": "^4.18.1"
  }
}
